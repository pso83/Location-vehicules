   -
     name : FicheVehicule
     procedure_id : 1215039350326942697
     type_code : 12
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] FicheVehicule ()
