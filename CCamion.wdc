   -
     name : Sauve
     procedure_id : 1215248708518847673
     type_code : 12
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // Sauve ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE VIRTUELLE Sauve():booléen
      bSucces est un booléen = Vrai
      
      SI :p_nIDVehicule = 0 _OU_ :p_nIDVehicule = "" _OU_ :p_nIDVehicule = Null ALORS
      	HRAZ(T_Vehicules)
      	:TypeDuVehicule()
      SINON
      	HLitRecherchePremier(T_Vehicules,IDT_Vehicules,:p_nIDVehicule)
      	SI HTrouve(T_Vehicules) = Faux ALORS
      		bSucces = Faux
      	FIN
      FIN
      
      SI bSucces = Vrai ALORS
      	// Spécificité camions
      	T_Vehicules.Poids		= :m_nPoids
      	
      	// Commun à tous les véhicules
      	T_Vehicules.Couleur			= :p_sCouleur
      	T_Vehicules.Kilometrage		= :p_nKilometrage
      	T_Vehicules.Immatriculation	= :p_sNumeroImmatriculation
      	T_Vehicules.Marque			= :p_sMarque
      	T_Vehicules.Modele			= :p_sModel
      	T_Vehicules.Transmission 	= :p_sTransmission
      	T_Vehicules.Energie 		= :p_sEnergie
      	T_Vehicules.NumeroMoteur	= :p_sNumeroMoteur
      	T_Vehicules.TypeVehicule	= :p_sTypeVehicule
      	
      	bSucces						= HEnregistre(T_Vehicules)
      	
      FIN
      
      SI bSucces _ET_ :p_nIDVehicule = 0 _OU_ :p_nIDVehicule = "" _OU_ :p_nIDVehicule = Null ALORS
      	:p_nIDVehicule = T_Vehicules.IDT_Vehicules
      FIN
      
      HFerme(T_Vehicules)
      RENVOYER bSucces
      
