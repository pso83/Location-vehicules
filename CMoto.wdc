     name : FicheVehicule
     procedure_id : 1215029549263620913
     type_code : 12
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] FicheVehicule ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	chaîne ANSI : // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      FONCTION VIRTUELLE FicheVehicule()
      
      sDispo est une chaîne
      SI p_bEstDispo ALORS
      	sDispo = "INDISPONIBLE"
      SINON
      	sDispo = "DISPONIBLE"
      FIN
      
      sMafiche est une chaine = [
      	Marque : [%p_sMarque%]
      	Modèle : [%p_sModel%]
      	Énergie : [%p_sEnergie%]
      	Numéro d'immatriculation : [%p_sNumeroImmatriculation%]
      	Cylindrée : [%p_nCylindre%] cm3
      	Couleur : [%p_sCouleur%]
      	Transmission : [%p_sTransmission%]
      	Kilométrage : [%p_nKilometrage%] km
      	Disponibilité : [%sDispo%]
      ]
      
      RENVOYER sMafiche
      
