   -
     name : Charge
     procedure_id : 1215030365307858812
     type_code : 12
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] Charge (<nPIDT_Moto> est entier sur 8 octets)
      //
      // Paramètres :
      //	nPIDT_Moto (entier sur 8 octets) : <indiquez ici le rôle de nPIDT_Moto>
      // Valeur de retour :
      // 	booléen : // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      FONCTION VIRTUELLE Charge(nPIDT_Moto est un entier sur 8 octets)
      
      bTrouve est un booléen = Vrai
      
      HLitRecherchePremier(T_Vehicules,IDT_Vehicules,nPIDT_Moto)
      SI HTrouve(T_Vehicules) ALORS
      	// Données de la classe CMoto
      	m_nCylindre = T_Vehicules.Cylindree
      	
      	// Données de la classe CVehicules
      	:p_sCouleur = T_Vehicules.Couleur
      	:p_nKilometrage = T_Vehicules.Kilometrage
      	:p_sMarque = T_Vehicules.Marque
      	:p_sModel = T_Vehicules.Modele
      	:p_sTransmission = T_Vehicules.Transmission
      	:p_sEnergie = T_Vehicules.Energie
      	:p_sTypeVehicule = T_Vehicules.TypeVehicule
      	:p_sNumeroMoteur = T_Vehicules.NumeroMoteur
      	:p_sNumeroImmatriculation = T_Vehicules.Immatriculation
      	:p_nIDVehicule = T_Vehicules.IDT_Vehicules
      	:p_bEstDispo = T_Vehicules.EstDispo
      	:p_Photo = T_Vehicules.Photo
      FIN
      
      HFerme(T_Vehicules)
      
      RENVOYER bTrouve
      
      
